# Local Development Environment Variables
# Copy this file to .env and fill in your values

# Database Connection
LLC_POSTGRES_DSN=postgresql://local:localpw@postgres:5432/registry

# Authentication
LLC_MCP_TOKEN=local-dev-token

# API Keys (Required for embeddings and graph features)
LLC_NOMIC_API_KEY=your-nomic-key-here
LLC_GOOGLE_API_KEY=your-google-key-here
LLC_OPENROUTER_API_KEY=your-openrouter-key-here

# Graph LLM Configuration
LLC_GRAPH_LLM_ENABLED=true
LLC_GRAPH_LLM_MODEL=qwen/qwen3-235b-a22b-thinking-2507

# Embedding Provider (nomic or google)
LLC_EMBEDDER_PROVIDER=nomic
